{
  "[nix]": {
    "editor.defaultFormatter": "B4dM4n.nixpkgs-fmt"
  },
  "[php]": {
    "editor.defaultFormatter": "bmewburn.vscode-intelephense-client"
  },
  "[rust]": {
    "editor.defaultFormatter": "rust-lang.rust-analyzer"
  },
  "rust-analyzer.check.command": "clippy",
  "breadcrumbs.enabled": false,
  "editor.codeActionsOnSave": {
    "quickfix.biome": "explicit",
    "source.organizeImports.biome": "explicit"
  },
  "editor.cursorSmoothCaretAnimation": "on",
  "editor.cursorSurroundingLines": 8,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.fontFamily": "JetBrainsMono Nerd Font",
  "editor.fontLigatures": true,
  "editor.fontSize": 14,
  "editor.formatOnSave": true,
  "editor.inlineSuggest.enabled": true,
  "editor.lineHeight": 1.8,
  "editor.linkedEditing": true,
  "editor.minimap.autohide": true,
  "editor.minimap.enabled": false,
  "editor.renderLineHighlight": "gutter",
  "editor.scrollbar.horizontal": "hidden",
  "editor.scrollbar.vertical": "hidden",
  "editor.semanticHighlighting.enabled": false,
  "editor.stickyScroll.enabled": true,
  "editor.wordSeparators": "`~!@#$%^&*()=+[{]}\\|;:'\",.<>/?",
  "explorer.fileNesting.enabled": true,
  "explorer.fileNesting.patterns": {
    "*.js": "${capture}.js.map, ${capture}.min.js, ${capture}.d.ts",
    "*.jsx": "${capture}.js",
    "*.ts": "${capture}.js",
    "*.tsx": "${capture}.ts ${capture}.*.tsx",
    ".env": ".env*",
    "composer.json": "composer.lock",
    "package.json": "package-lock.json, yarn.lock, pnpm-lock.yaml, bun.lockb",
    "tailwind.config.*": "tailwind.config*, postcss.config*",
    "tsconfig.json": "tsconfig.*.json"
  },
  "extensions.autoCheckUpdates": false,
  "git.enableSmartCommit": true,
  "github.copilot.nextEditSuggestions.enabled": true,
  "github.copilot.enable": {
    "*": true,
    "markdown": true,
    "plaintext": false,
    "scminput": false
  },
  "github.copilot.editor.enableAutoCompletions": true,
  "github.copilot.nextEditSuggestions.fixes": true,
  "github.copilot.selectedCompletionModel": "gpt-4o-copilot",
  "security.workspace.trust.emptyWindow": false,
  "security.workspace.trust.enabled": false,
  "security.workspace.trust.untrustedFiles": "open",
  "symbols.hidesExplorerArrows": false,
  "terminal.external.linuxExec": "alacritty",
  "terminal.external.osxExec": "alacritty.app",
  "terminal.integrated.copyOnSelection": true,
  "terminal.integrated.defaultProfile.linux": "tmux-shell",
  "terminal.integrated.defaultProfile.osx": "tmux-shell",
  "terminal.integrated.fontFamily": "FiraCode Nerd Font",
  "terminal.integrated.macOptionClickForcesSelection": true,
  "terminal.integrated.profiles.linux": {
    "tmux-shell": {
      "args": ["new-session", "-A", "-s", "vscode:${workspaceFolder}"],
      "path": "tmux"
    }
  },
  "terminal.integrated.profiles.osx": {
    "tmux-shell": {
      "args": ["new-session", "-A", "-s", "vscode:${workspaceFolder}"],
      "path": "tmux"
    }
  },
  "terminal.integrated.shellIntegration.enabled": true,
  "typescript.preferences.importModuleSpecifier": "non-relative",
  "update.mode": "none",
  "vim.easymotion": true,
  "vim.foldfix": true,
  "vim.handleKeys": {
    "<C-a>": false,
    "<C-c>": false,
    "<C-n>": false,
    "<C-p>": false,
    "<C-w>": false
  },
  "vim.highlightedyank.color": "rgba(230, 97, 89, 0.7)",
  "vim.highlightedyank.enable": true,
  "vim.highlightedyank.textColor": "white",
  "vim.hlsearch": true,
  "vim.incsearch": true,
  "vim.insertModeKeyBindings": [
    {
      "after": ["<Esc>"],
      "before": ["j", "k"]
    }
  ],
  "vim.leader": "<space>",
  "vim.normalModeKeyBindingsNonRecursive": [
    {
      "before": ["<leader>", "r"],
      "commands": ["editor.action.rename"]
    },
    {
      "before": ["<leader>", "f"],
      "commands": ["editor.action.wordHighlight.next"]
    },
    {
      "before": ["<leader>", "|"],
      "commands": ["workbench.action.splitEditor"]
    },
    {
      "before": ["<leader>", "h"],
      "commands": ["workbench.action.focusLeftGroup"]
    },
    {
      "before": ["H"],
      "commands": ["workbench.action.previousEditor"]
    },
    {
      "before": ["L"],
      "commands": ["workbench.action.nextEditor"]
    },
    {
      "before": ["<leader>", "l"],
      "commands": ["workbench.action.focusRightGroup"]
    },
    {
      "before": ["u"],
      "commands": ["undo"]
    },
    {
      "before": ["U"],
      "commands": ["redo"]
    },
    {
      "before": ["g", "p", "d"],
      "commands": ["editor.action.peekDefinition"]
    },
    {
      "before": ["g", "h"],
      "commands": ["editor.action.showDefinitionPreviewHover"]
    },
    {
      "before": ["g", "i"],
      "commands": ["editor.action.goToImplementation"]
    },
    {
      "before": ["g", "p", "i"],
      "commands": ["editor.action.peekImplementation"]
    },
    {
      "before": ["g", "r"],
      "commands": ["editor.action.referenceSearch.trigger"]
    },
    {
      "before": ["g", "t"],
      "commands": ["editor.action.goToTypeDefinition"]
    },
    {
      "before": ["g", "p", "t"],
      "commands": ["editor.action.peekTypeDefinition"]
    },
    {
      "before": ["g", "e"],
      "commands": ["editor.action.marker.next"]
    },
    {
      "before": ["q", "f"],
      "commands": ["editor.action.quickFix"]
    },
    {
      "after": ["1", "5", "g", "j", "z", "z"],
      "before": ["<C-d>"]
    },
    {
      "after": ["1", "5", "g", "k", "z", "z"],
      "before": ["<C-u>"]
    },
    {
      "before": ["K"],
      "commands": ["lineBreakInsert"],
      "silent": true
    }
  ],
  "vim.smartRelativeLine": true,
  "vim.useCtrlKeys": true,
  "vim.useSystemClipboard": true,
  "window.commandCenter": false,
  "window.customTitleBarVisibility": "never",
  "window.titleBarStyle": "native",
  "window.zoomLevel": 0,
  "workbench.activityBar.location": "bottom",
  "workbench.colorTheme": "Ayu Dark",
  "workbench.iconTheme": "symbols",
  "workbench.layoutControl.enabled": false,
  "workbench.sideBar.location": "right",
  "workbench.startupEditor": "readme",
  "workbench.statusBar.visible": true
}
